package javaEx;

public class BoxingEx {

	public static void main(String[] args) {
		// boolean, byte, short, int, char
		// long
		// float, double
		// 같은 스펠링에 대문자로 시작하는 클래스
		// Boolean, Byte, Sort, Integer, Character
		// Long, Float, Double
		int a = 20;
		Integer aa = a; // boxing
		int c = aa; // unboxing
		Integer cc = new Integer(20);
		System.out.println(a);
	}
}
