package javaEx;

public class WhileEx1 {
	public static void main(String[] args) {
		//While은 조건만 있음 반복문, 초기값과 증가값 필요
		// 1~10 출력하는 루틴
		
		int i = 1;
		while(i<=10) {
			System.out.println(i);
			i++; // 이게 없으면 무한루프.
		}
		//do-while 실행을 먼저 하고, 조건을 비교
		int v = 1;
		do-while(v<10) {
			System.out.println(v);
			v++;
		}
	}
}
